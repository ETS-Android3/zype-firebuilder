{
  "cooker": "DynamicParser",
  "format": "json",
  "model": "com.amazon.android.model.content.ContentContainer",
  "translator": "ZypeContentContainerTranslator",
  "modelType": "single",
  "query": "$.categories[?(@.parent_id in [$$par0$$])]",
  "matchList": [
    "title@name",
    "parent_id@parentId",
    "thumbnails@thumbnails"
  ],
  "keyDataType": "_id@keyDataPath"
}